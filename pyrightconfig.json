{
  "executionEnvironments": [
    {
      "root": "lib",
      "extraPaths": [
        "lib/site-packages",
      ]
    },
    {
      "root": "tests",
      "extraPaths": [
        "lib/site-packages"
      ],
    }
  ],
  "typeCheckingMode": "basic",
  "reportMissingModuleSource": "none",
  "reportUnusedImport": "error",
  "reportUnusedCallResult": "error",
  "reportUnknownMemberType": "information",
  "include": [
    "lib",
    "*.py"
  ],
  "exclude": [
    "lib/site-packages",
  ],
}
